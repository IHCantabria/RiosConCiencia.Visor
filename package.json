{
  "name": "riosconciencia.visor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:dev": "vue-cli-service build --mode development",
    "build": "vue-cli-service build",
    "build:all": "vue-cli-service build && vue-cli-service build --mode development",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:style": "vue-cli-service lint:style",
    "lint:code": "vue-cli-service lint",
    "stylelint-check": "stylelint-config-prettier-check",
    "deploy-patch": "npm version patch && npm run build:all && git add . && git commit -m",
    "deploy-minor": "npm version minor && npm run build:all && git add . && git commit -m",
    "deploy-major": "npm version major && npm run build:all && git add . && git commit -m",
    "postgit": "git push origin master && git push origin --tags",
    "postgit:patch": "git push origin master",
    "test": "npm run lint && vue-cli-service test:unit --no-cache && vue-cli-service test:e2e --headless",
    "test:e2e": "vue-cli-service test:e2e",
    "test:snap": "npm run test:unit -- -u",
    "test:unit": "vue-cli-service test:unit --no-cache",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@saeris/vue-spinners": "^1.0.8",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "leaflet-easybutton": "^2.4.0",
    "leaflet-extra-markers": "^1.2.1",
    "material-icons": "^0.3.1",
    "sweet-modal-vue": "^2.0.0",
    "vue": "^2.6.11",
    "vuesax": "^3.11.15",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-e2e-cypress": "3.11.0",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^5.0.0",
    "flush-promises": "^1.0.2",
    "jest-css-modules": "^2.1.0",
    "lodash.merge": "^4.6.2",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.17.2",
    "vue-template-compiler": "^2.6.11"
  }
}
