{
  "name": "riosconciencia.visor",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:all": "vue-cli-service build && vue-cli-service build --mode development",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:style": "stylelint src/**/*.{vue,htm,html,css,sss,less,scss}",
    "lint:code": "vue-cli-service lint",
    "stylelint-check": "stylelint-config-prettier-check",
    "test": "npm run test:unit -- --passWithNoTests && npm run test:e2e -- --headless",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit --no-cache",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "test:snap": "npm run test:unit -- -u",
    "review": "npm run lint && npm run test",
    "deploy-patch": "npm version patch -m v%s",
    "deploy-minor": "npm version minor -m v%s",
    "deploy-major": "npm version major -m v%s",
    "preversion": "npm run review",
    "version": "npm run build:all && git add -A",
    "postversion": "git push origin main && git push origin --tags",
    "preinstall": "npx npm-force-resolutions"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@saeris/vue-spinners": "^1.0.8",
    "axios": "^0.21.1",
    "core-js": "^3.8.2",
    "leaflet": "1.5.1",
    "leaflet-easybutton": "^2.4.0",
    "leaflet-extra-markers": "^1.2.1",
    "leaflet-groupedlayercontrol": "^0.6.1",
    "material-icons": "^0.5.2",
    "sweet-modal-vue": "^2.0.0",
    "vue": "^2.6.12",
    "vuesax": "^3.11.15",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-e2e-cypress": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-macros": "^3.0.1",
    "cypress-wait-until": "^1.7.1",
    "eslint": "^7.17.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.4.1",
    "flush-promises": "^1.0.2",
    "jest-css-modules": "^2.1.0",
    "lodash.merge": "^4.6.2",
    "node-sass": "^5.0.0",
    "npm-force-resolutions": "^0.0.3",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.1.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "vue-template-compiler": "^2.6.12"
  },
  "resolutions": {
    "lodash": "^4.17.19",
    "minimist": "^1.2.3",
    "yargs-parser": "^18.1.2"
  }
}
